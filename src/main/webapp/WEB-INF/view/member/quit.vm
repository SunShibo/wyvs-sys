<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            <span style="color: #d30d42;font-size: 26px;font-weight: bold;">工作人员辞职报告</span>
        ##                <small>advanced tables</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Member Quit</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="box">
            <div class="box-header">
                <span>申请人信息</span>
            </div><!-- /.box-header -->

            <div class="box-body">

                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>姓名</label>
                            <p class="form-control-static">$!{member.name}/$!{member.englishName}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>部门</label>
                            <p class="form-control-static">$!{member.department}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>申请时间</label>
                            <p class="form-control-static">$!{member.jobGrade}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->

                    <div class="col-md-3">
                        <div class="form-group">
                            <label>UUID</label>
                            <p class="form-control-static">$!{member.nationality}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                </div><!-- /.row -->

            </div><!-- /.box-body -->
        </div><!-- /.box -->
        <div class="box box-primary">
            <div class="box-header">
                <span>离职人信息</span>
            </div><!-- /.box-header -->

            <div class="box-body">

                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>姓名</label>
                            <p class="form-control-static">$!{member.name}/$!{member.englishName}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>职务/职级</label>
                            <p class="form-control-static">$!{member.title}/$!{member.jobGrade}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>部门</label>
                            <p class="form-control-static">测试部门</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>加入时间</label>
                            <p class="form-control-static">$!{member.join_time}</p>
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                </div><!-- /.row -->

                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>最后工作日</label>
                            <input id="last_date" class="form-control pull-right active" name="lastDate"
                                   placeholder="last date" type="text">
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                    <div class="col-md-9">
                        <div class="form-group">
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                </div><!-- /.row -->
                <div class="row">
                    <div class="col-md-9">
                        <div class="form-group">
                        </div><!-- /.form-group -->
                    </div><!-- /.col -->
                </div><!-- /.row -->
                <div class="row">
                    <div class="col-md-6">
                        <!-- textarea -->
                        <div class="form-group">
                            <label>辞职理由陈述</label>
                            <textarea class="form-control" rows="3" name="remarks" placeholder="Enter ..."></textarea>
                        </div>
                    </div><!-- /.col -->

                </div><!-- /.row -->

            </div><!-- /.box-body -->
        </div><!-- /.box -->

    </section><!-- /.content -->
</div><!-- /.content-wrapper -->

<script type="text/javascript">

    $(function(){
        //Date range picker
        $('#last_date').datepicker({
            format: 'yyyy-mm-dd',
            language: 'cn'
        });
    });
</script>

